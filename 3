# Option 1: Start from scratch
mkdir github-branch-lab
cd github-branch-lab
git init

echo "print('Hello from Git!')" > hello.py
git add hello.py
git commit -m "Initial commit"

git remote add origin https://github.com/USERNAME/github-branch-lab.git
git branch -M main
git push -u origin main

git checkout -b feature-1
echo "print('Feature-1 branch edit')" >> hello.py
git add hello.py
git commit -m "Edited hello.py in feature-1"
git push -u origin feature-1
